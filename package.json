{
    "name": "Fliwer",
    "version": "2.0.0",
    "description": "Fliwer website app. Powered by React native web",
    "main": "web/entry.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "webpack serve",
        "android": "cd MyFliwer && react-native run-android",
        "ios": "cd MyFliwer && react-native run-ios",
        "ios-device": "cd MyFliwer && react-native run-ios --device",
        "build-android": "cd MyFliwer/android && gradlew.bat assembleRelease  && gradlew.bat bundleRelease && adb install ./app/build/outputs/apk/release/app-release.apk",
        "build-android-linux": "cd MyFliwer/android && ./gradlew assembleRelease && ./gradlew bundleRelease && adb install ./app/build/outputs/apk/release/app-release.apk",
        "install-android": "adb install ./MyFliwer/android/app/build/outputs/apk/release/app-release.apk",
        "build-ios": "cd MyFliwer && react-native bundle --reset-cache --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --assets-dest=ios --platform='ios'",
        "build-web-linux": "build-web.sh",
        "build-web-windows": "build-web.bat",
        "push": " node push-web.js",
        "upload-web": "node upload-web.js",
        "electron": "webpack && cd electron && npm start"
    },
    "rnpm": {
        "assets": [
            "./assets/fonts/"
        ]
    },
    "author": "Inolve Newtech",
    "license": "ISC",
    "homepage": "https://www.fliwer.com",
